fn void utf8_slice() @test
{
	UTF8String str = @utf8("ababc😀😀c😀de😀fgh");
	test::eq(str.slice(0, 0), @utf8("a"));
	test::eq(str.slice(0, 5), @utf8("ababc😀"));
	test::eq(str.slice(4, 6), @utf8("c😀😀"));
	test::eq(str.slice(str.len() - 1, str.len() - 1), @utf8("h"));
	test::eq(str.slice(0, str.len() - 1), str);
}

fn void utf8_slice_len() @test
{
	UTF8String str = @utf8("ababc😀😀c😀de😀fgh");
	test::eq(str.slice_len(0, 0), @utf8(""));
	test::eq(str.slice_len(1, 5), @utf8("babc😀"));
	test::eq(str.slice_len(4, 6), @utf8("c😀😀c😀d"));
	test::eq(str.slice_len(0, str.len()), str);
	test::eq(str.slice_len(1, 1), @utf8("b"));
}

fn void utf8_contains() @test
{
	UTF8String str = @utf8("ababc😀😀c😀de😀fgh");
	assert(str.contains(@utf8("bc😀")));
